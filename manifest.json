{
  "name": "RealMe",
  "short_name": "RealMe",
  "description": "Conecte-se com amigos e encontre novas pessoas no RealMe!",
  "start_url": "/feed.html",
  "scope": "/",
  "id": "/manifest.json",
  "display": "standalone",
  "display_override": ["standalone", "fullscreen"],
  "background_color": "#000000",
  "theme_color": "#4A90E2",
  "orientation": "portrait-primary",
  "categories": ["social", "networking", "lifestyle"],
  "shortcuts": [
    {
      "name": "Novo Post",
      "short_name": "Post",
      "description": "Crie um novo post rapidamente",
      "url": "/feed",
      "icons": [
        {
          "src": "./src/icon/shortcut-post.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Perfil",
      "short_name": "Perfil",
      "description": "Veja seu perfil",
      "url": "/PF",
      "icons": [
        {
          "src": "./src/icon/shortcut-friends.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    }
  ],
  "related_applications": [
    {
      "platform": "play",
      "url": "https://play.google.com/store/apps/details?id=com.ifriendmatch.app",
      "id": "com.ifriendmatch.app"
    },
    {
      "platform": "itunes",
      "url": "https://apps.apple.com/app/id123456789"
    }
  ],
  "prefer_related_applications": false,
  "iarc_rating_id": "e10c-1234-5678-9abc",
  "launch_handler": {
    "client_mode": "navigate-new"
  },
  "protocol_handlers": [
    {
      "protocol": "web+ifriend",
      "url": "/protocol-handler?url=%s"
    }
  ],
  "permissions_policy": {
    "geolocation": [],
    "camera": [],
    "microphone": []
  },
  "dir": "ltr",
  "lang": "pt-BR"
}